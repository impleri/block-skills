  "authors": [
    "impleri"
  ],
  "contact": {
    "homepage": "https://github.com/impleri/block-skills",
    "sources": "https://github.com/impleri/block-skills",
    "issues": "https://github.com/impleri/block-skills/issues"
  },
  "license": "MIT",
  "icon": "assets/blockskills/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.impleri.blockskills.fabric.BlockSkillsFabric"
    ]
  },
  "mixins": [
    "blockskills-common.mixins.json",
    "blockskills.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": "1.19.2",
    "architectury": ">=6.2.0",
    "playerskills": ">=1.7.2"
